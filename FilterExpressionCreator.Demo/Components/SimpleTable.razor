@typeparam TItem
@inherits FreelancerTableComponent<TItem>
<div class="table-responsive">
    <table class="table table-striped table-sm">
        <thead>
            @HeaderTemplate
        </thead>
        <tbody>
            @foreach (var item in Items)
            {
                @RowTemplate(item)
            }
        </tbody>
    </table>
</div>